{
    "name": "graymatterlabs/pingtree",
    "description": "A Ping Tree is a process in which a single lead is offered to multiple buyers in real-time and sold to the highest bidder",
    "keywords": [
        "graymatterlabs",
        "pingtree"
    ],
    "homepage": "https://github.com/graymatterlabs/pingtree",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Colson",
            "email": "ryan@graymatterlabs.co"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "graymatterlabs/simple-cache": "^1.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "psr/simple-cache": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GrayMatterLabs\\PingTree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrayMatterLabs\\PingTree\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "vendor/bin/phpstan analyze",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-text",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
